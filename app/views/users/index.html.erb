<div class="container">
<h1>Users#index</h1>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">User Name</th>
          <th scope="col">Email</th>
          <th scope="col">Password</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <th scope="row"><%= user.id %></th>
            <td><%= link_to user.username, "/users/#{user.id}" %></td>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
</div>
